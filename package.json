{
  "name": "keyboard-vis",
  "version": "0.0.1",
  "description": "Keyboard layout visualization",
  "keywords": [
    "keyboard shortcuts"
  ],
  "homepage": "https://github.com/Dmitra/keyboard#readme",
  "license": "agpl-3.0",
  "author": {
    "name": "Adrian Dmitra",
    "email": "adrian@dmitra.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dmitra/keyboard"
  },
  "scripts": {
    "start": "webpack-dev-server --watch --config webpack.config.js"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
