{
  "name": "keyboard-vis",
  "version": "0.0.1",
  "description": "Keyboard layout visualization",
  "keywords": [
    "keyboard shortcuts"
  ],
  "homepage": "https://github.com/Dmitra/keyboard#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dmitra/keyboard"
  },
  "license": "agpl-3.0",
  "author": {
    "name": "Adrian Dmitra",
    "email": "adrian@dmitra.com"
  },
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack-dev-server --inline",
    "deploy": "rsync -azP --delete dist/ data src/index.html daviste.com:/home/dmitra/daviste.com/static/demo/keyboard-layout"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-runtime"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ],
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "@graphiy/eslint-config",
    "globals": {
      "_": true,
      "$": true
    }
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@mdi/font": "^4.5.95",
    "jquery": "^3.5.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.11",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.3",
    "@graphiy/eslint-config": "^0.1.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^3.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "js-yaml-loader": "^1.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-jss": "^8.6.1",
    "style-loader": "^1.0.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  }
}
